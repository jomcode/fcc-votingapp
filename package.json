{
  "name": "votingapp",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test:server": "better-npm-run test:server",
    "test:server:watch": "better-npm-run test:server:Watch",
    "dev:server": "better-npm-run dev:server",
    "dev:client": "better-npm-run dev:client",
    "dev": "better-npm-run dev",
    "build:client": "better-npm-run build:client",
    "start": "npm run build:client && node index.js"
  },
  "betterScripts": {
    "dev:server": {
      "command": "nodemon --ignore src/client.js --ignore src/client index.js",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "dev:client": {
      "command": "webpack-dev-server",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "dev": {
      "command": "concurrently -k \"npm run dev:server\" \"npm run dev:client\"",
      "env": {
        "NODE_ENV": "developemnt"
      }
    },
    "build:client": {
      "command": "webpack",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "test:server": {
      "command": "mocha --recursive",
      "env": {
        "NODE_ENV": "test"
      }
    },
    "test:server:watch": {
      "command": "nodemon --exec mocha --recursive",
      "env": {
        "NODE_ENV": "test"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jomcode/fcc-votingapp.git"
  },
  "keywords": [],
  "author": "John <jomcode@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jomcode/fcc-votingapp/issues"
  },
  "homepage": "https://github.com/jomcode/fcc-votingapp#readme",
  "dependencies": {
    "babel-polyfill": "^6.7.4",
    "bcryptjs": "^2.3.0",
    "body-parser": "^1.15.0",
    "chart.js": "^1.1.1",
    "classnames": "^2.2.5",
    "cors": "^2.7.1",
    "express": "^4.13.4",
    "express-jwt": "^3.3.0",
    "jsonwebtoken": "^5.7.0",
    "mongoose": "^4.4.13",
    "morgan": "^1.7.0",
    "normalize.css": "^4.1.1",
    "passport": "^0.3.2",
    "passport-jwt": "^2.0.0",
    "react": "^15.0.1",
    "react-addons-css-transition-group": "^15.0.2",
    "react-chartjs": "^0.7.3",
    "react-dom": "^15.0.1",
    "react-redux": "^4.4.5",
    "react-redux-notifications": "^0.2.1",
    "react-router": "^2.4.0",
    "react-router-redux": "^4.0.4",
    "redux": "^3.5.2",
    "redux-form": "^5.2.3",
    "redux-thunk": "^2.0.1",
    "reselect": "^2.5.1",
    "whatwg-fetch": "^1.0.0"
  },
  "engines": {
    "node": "4.4.3"
  },
