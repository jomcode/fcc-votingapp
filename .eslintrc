{
  "extends": "airbnb",

  "parser": "babel-eslint",

  "globals": {},

  "env": {
    "mocha": true
  },

  "rules": {
    "no-unused-vars": [1],
    "comma-dangle": [0],
    "brace-style": [0],
    "no-console": [0],
    "dot-notation": [0],
    "no-param-reassign": [1],
    "new-cap": [0],
    "vars-on-top": [0],
    "global-require": [0],
    "no-underscore-dangle": [0],
    "no-confusing-arrow": [0]
  }
}
